.header {
    font-size: 22px;
    -unity-font-style: bold;
    color: rgb(27, 27, 27);
    -unity-text-align: middle-center;
    background-color: rgba(94, 94, 94, 0.5);
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 5px;
    margin-bottom: 5px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    border-top-left-radius: 15px;
    border-bottom-left-radius: 15px;
    border-top-right-radius: 15px;
    border-bottom-right-radius: 15px;
    width: 250px;
}

.toogle {
    font-size: 20px;
}

.toogle:hover {
    font-size: 10px;
}

.container {
    flex-direction: row;
    flex-wrap: wrap;
    align-items: center;
    flex-grow: 1;
    background-color: rgb(94, 94, 94);
    justify-content: flex-start;
    align-self: flex-start;
    padding-left: 59px;
    padding-right: 59px;
    padding-top: 59px;
    padding-bottom: 59px;
    margin-left: 8px;
    margin-right: 8px;
    margin-top: 8px;
    margin-bottom: 8px;
}

.outercontainer {
    flex-direction: row;
    flex-wrap: wrap;
}
